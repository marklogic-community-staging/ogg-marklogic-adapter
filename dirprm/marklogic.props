
gg.handlerlist=marklogic

gg.handler.marklogic.type=oracle.goldengate.delivery.handler.marklogic.MarkLogicHandler

#MarkLogic Host
gg.handler.marklogic.host=localhost
#Default port for connecting to MarkLogic
gg.handler.marklogic.port=8000
gg.handler.marklogic.database=Documents
gg.handler.marklogic.user=admin
gg.handler.marklogic.password=admin
gg.handler.marklogic.auth=digest
gg.handler.marklogic.format=json
gg.handler.marklogic.rootName=root

# Use these to configure a transform to run
# gg.handler.marklogic.transformName=<transform name>
# gg.handler.marklogic.transformParams=<param1=paramvalue1,param2=paramvalue2,...>

# The following are an example of running a data hub framework input flow
# gg.handler.marklogic.transformName=mlRunIngest
# gg.handler.marklogic.transformParams=entity-name=MyEntity,flow-name=myflowname,job-id=myjobid,step=1

# Don't had job id if you can't  genereate it randomly the flow will already do it for you
# gg.handler.marklogic.transformParams=entity-name=MyEntity,flow-name=myflowname,step=1

gg.handler.marklogic.mode=tx

# to keeps original raw data collumn names
gg.handler.marklogic.rawName=true

# prefix for uri creation
gg.handler.marklogic.uriPrefix=http://marklogic.nl

# add schema name to document path
gg.handler.marklogic.addSchema=false

# defines null values coming from dbs as emptry string instead of null
gg.handler.marklogic.emptyString=true

goldengate.userexit.timestamp=utc
goldengate.userexit.writers=javawriter
javawriter.stats.display=TRUE
javawriter.stats.full=TRUE

gg.log=log4j
gg.log.level=DEBUG

gg.report.time=30sec

##CHANGE THE PATH BELOW TO ADAPTER JAR directory
gg.classpath=/home/joe/ggs/marklogic/target/lib/*

javawriter.bootoptions=-Xmx512m -Xms32m -Djava.class.path=ggjava/ggjava.jar:

